import {SET_BREADCRUMS_VALUE,actionPaylodType,BreadcrumsState,breadcrumsActionType} from './types';

const breadCrumsInitialState: BreadcrumsState = {
		value: ""
	};

const breadcrumsReducer = (state : BreadcrumsState =breadCrumsInitialState, action :breadcrumsActionType) => {
	switch(action.type){
		case SET_BREADCRUMS_VALUE:{
			return {
				...state,
				value : action.payload.value
			}
		}

	}
}
